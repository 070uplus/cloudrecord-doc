---
title: search
layout: none
search: exclude
---

{
{% for post in site.pages %}
{% unless page.search == "exclude" %}
"{{ post.url | slugify }}": {
"title": "{{ post.title | xml_escape }}",
"url": " {{ post.url | xml_escape | prepend: '..'}}",
"summary": {{post.summary}},
"body": {{post.content}},
"tags": "{{ post.tags | xml_escape }}"
}
{% endunless %}
{% unless forloop.last %},{% endunless %}
{% endfor %}
}